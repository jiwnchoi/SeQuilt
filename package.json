		"esbuild": "^0.21.4",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"typescript": "^5.4.5",
		"vite": "^5.3.1"
	},
	"dependencies": {
		"d3": "^7.9.0",
		"lit-html": "^3.1.4"
